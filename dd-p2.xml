<packages xmlns="http://dd-p2.dflydev.com/2010">
    <package>
        <version>0.1</version>
        <name>Dragonfly Development jQuery Namespace Add-on</name>
        <url>http://github.com/dflydev/jquery-ddNamespace</url>
        <copyright>Copyright (c) 2010 Dragonfly Development</copyright>
        <license>Licensed under the New BSD License.</license>
        <files>
            <file name="README" />
            <file name="LICENSE.txt" />
            <file name="dflydev-namespace" type="javascript" includeVersion="true" />
            <file name="jquery-ddNamespace" type="javascript" includeVersion="true" />
            <combined name="jquery-ddNamespace-full" type="javascript" minifiedHeader="generated" includeVersion="true">
                <file name="dflydev-namespace" />
                <file name="jquery-ddNamespace" />
            </combined>
        </files>
        <outputs>
            <output name="jquery-ddNamespace-full">
                <includeFile name="README" />
                <includeFile name="LICENSE.txt" />
                <includeFile name="dflydev-namespace" />
                <includeFile name="jquery-ddNamespace" />
                <includeFile name="jquery-ddNamespace-full" />
            </output>
            <output name="jquery-ddNamespace">
                <includeFile name="README" />
                <includeFile name="LICENSE.txt" />
                <includeFile name="jquery-ddNamespace-full" as="jquery-ddNamespace" />
            </output>
            <output name="dflydev-namespace">
                <includeFile name="README" />
                <includeFile name="LICENSE.txt" />
                <includeFile name="dflydev-namespace" />
            </output>
        </outputs>
    </package>
</packages>
